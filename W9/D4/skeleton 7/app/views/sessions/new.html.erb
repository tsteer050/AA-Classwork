<h3>Log In, Bro</h3>

<form action="<%= session_url %>" method="post">

  <label for="user_username">Username</label>
    <input 
    id="user_username"
    name="user[user_name]" 
    type="text"
    >
  <br>
  <label for="user_password">Password</label>
    <input 
    id="user_password"
    name="user[password]" 
    type="password"
     >
  <br>
    <input type="submit" value="Log In">


</form>