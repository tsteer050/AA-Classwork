<h2>New Link</h2>


<form action="<%= links_url %>", method="POST">
  <label>
    Title
    <input type="text" name="link[title]" value="<%= @link.title %>">
  </label>
  <label>
    URL
    <input type="text" name="link[url]" value="<%= @link.url %>">
  </label>
  <input type="submit" value="Create New Link">

</form>